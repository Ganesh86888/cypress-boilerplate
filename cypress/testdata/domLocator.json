{
    "homePage":{
        "createAccount":"div[class*='panel header'] > ul > li:nth-child(4) > a",
        "signinAccount":"div[class*='panel header'] > ul > li:nth-child(2) > a"
    },
    "SignupPage":{
        "createAccountTitle":"h1[class='page-title'] > span",
        "firstNameTextBox":"#firstname",
        "lastNameTextBox":"#lastname",
        "emailAddressField":"#email_address",
        "passwordField":"#password",
        "passwordStrengthMeter":"#password-strength-meter",
        "passwordConfirmationField":"#password-confirmation",
        "createButton":"button[title='Create an Account']",
        "errorMessage":{
            "firstNameError":"#firstname-error",
            "emailAddressError":"#email_address-error",
            "lastNameError":"#lastname-error",
