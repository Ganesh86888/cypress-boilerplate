            "confirmPasswordError":"#password-confirmation-error",
            "passwordMeterLabel":"#password-strength-meter-label"
        },
        "registrationConfirmationAlert":"div[class='messages'] > div > div",
        "verifyCustomerName":"div[class='panel header'] > ul > li:nth-child(1) > span",
        "accountInformation":"div[class='block block-dashboard-info'] > div > strong",
        "contactInformation":"div[class='box box-information'] > strong > span"

    },
    "SignInPage":{
        "signInPageTitle":"h1[class='page-title'] > span",
        "signInButton":"fieldset[class='fieldset login'] button",
        "emailAddressField":"#email",
        "passwordField":"#pass",
        "verifyCustomerName":"div[class='panel header'] > ul > li:nth-child(1) > span"
    }
}